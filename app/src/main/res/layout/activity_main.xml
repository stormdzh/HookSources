<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvAndroidId"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFE1FF"
            android:gravity="center"
            android:text="AndroidId"
            android:textColor="#000000"
            android:textSize="14sp" />


        <TextView
            android:id="@+id/tvAndroidId2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#F5F5DC"
            android:gravity="center"
            android:text="AndroidId2"
            android:textColor="#000000"
            android:textSize="14sp" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/tvMAC"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#EE9A00"
            android:gravity="center"
            android:text="MAC"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvMAC1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#A4D3EE"
            android:gravity="center"
            android:text="MAC1"
            android:textColor="#000000"
            android:textSize="14sp" />


        <TextView
            android:id="@+id/tvIMSI"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#EE9A00"
            android:gravity="center"
            android:text="IMSI"
            android:textColor="#000000"
            android:textSize="14sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/tvIMEI"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFE1FF"
            android:gravity="center"
            android:text="IMEId"
            android:textColor="#000000"
            android:textSize="14sp" />


        <TextView
            android:id="@+id/tvIMEI1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#EE9A00"
            android:gravity="center"
            android:text="IMEI1d0"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvIMEI2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#F5F5DC"
            android:gravity="center"
            android:text="IMEI0"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvIMEI3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#EE9A00"
            android:gravity="center"
            android:text="IMEI1"
            android:textColor="#000000"
            android:textSize="14sp" />


        <TextView
            android:id="@+id/tvIMEI4"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#F5F5DC"
            android:gravity="center"
            android:text="IMEI反射"
            android:textColor="#000000"
            android:textSize="14sp" />


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvRecentTasks"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFE1FF"
            android:gravity="center"
            android:text="RecentTasks"
            android:textColor="#000000"
            android:textSize="14sp" />


        <TextView
            android:id="@+id/tvRuningTasks"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#F5F5DC"
            android:gravity="center"
            android:text="RuningTasks"
            android:textColor="#000000"
            android:textSize="14sp" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvClipboard"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFE1FF"
            android:gravity="center"
            android:text="剪切板"
            android:textColor="#000000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvLocation"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#F5F5DC"
            android:gravity="center"
            android:text="位置信息"
            android:textColor="#000000"
            android:textSize="14sp" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvSERIAL"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFE1FF"
            android:gravity="center"
            android:text="SERIAL"
            android:textColor="#000000"
            android:textSize="14sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvPrivacyInfo"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFE1FF"
            android:gravity="center"
            android:text="隐私信息"
            android:textColor="#000000"
            android:textSize="14sp" />




    </LinearLayout>


</LinearLayout>